{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "description": "A simple interface to communicate with Idylis",
  "keywords": [
    "idylis",
    "api",
    "interface"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsdx build",
    "test": "npm run-script init-test",
    "test:dev": "clear && tsdx build && npm test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "init-test": "scripts/create-credentials-env-var.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org:virtuoworks/idylisapi.git",
    "email": "contact@virtuoworks.com"
  },
  "contributors": [
    {
      "name": "Virtuoworks",
      "url": "https://www.virtuoworks.com/"
    },
    {
      "name": "Sami Radi",
      "email": "sami.radi@virtuoworks.com",
      "url": "https://github.com/Sami-Radi"
    },
    {
      "name": "Alexandre Masson",
      "email": "alexandre.masson@virtuoworks.com",
      "url": "https://github.com/Alexandre-Masson"
    }
  ],
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@virtuoworks/idylisapi",
  "author": "Virtuoworks",
  "homepage": "https://github.com/virtuoworks/idylisapi#readme",
  "module": "dist/idylisapi.esm.js",
  "size-limit": [
    {
      "path": "dist/idylisapi.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/idylisapi.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "fast-xml-parser": "^3.19.0",
    "typescript": "^4.2.4",
    "winston": "^3.3.3",
    "soap": "^0.38.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^7.0.1",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  }
}
